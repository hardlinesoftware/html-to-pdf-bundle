{
    "name": "hardlinesoftware/html-to-pdf-bundle",
    "type": "symfony-bundle",
    "description": "Convert HTML to PDF in Symfony",
    "require": {
        "php": ">=5.4",

        "symfony/process": "*",
        "symfony/dependency-injection": "*",
        "symfony/http-foundation": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "mikey179/vfsStream": "1.2.*"
    },
    "suggest": {
        "hardlinesoftware/html-to-pdf-bundle-driver-mpdf": "Generate PDFs using the mPDF driver"
    },
    "authors": [
        {
            "name": "Adam Quaile",
            "email": "adamquaile@gmail.com"
        }
    ],
    "keywords": ["pdf", "pdf-to-html"],
    "autoload": {
        "psr-4": {
            "EP\\Bundle\\HtmlToPdfBundle\\": ["", "tests/"]
        }
    }
}
